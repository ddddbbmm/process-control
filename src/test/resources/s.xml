<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="io.ddbm.pc.factory.FlowFactory">
        <property name="path" value="flow"/>
    </bean>

    <bean id="initAction" class="io.ddbm.pc.MockAction"/>
    <bean id="cancelAction" class="io.ddbm.pc.MockAction"/>
    <bean id="submitShenPiAction" class="io.ddbm.pc.MockAction"/>
    <bean id="shenPiQueryAction" class="io.ddbm.pc.MockAction"/>
    <bean id="passAction" class="io.ddbm.pc.MockAction"/>
    <bean id="rejectAction" class="io.ddbm.pc.MockAction"/>

    <bean id="applyContextService" class="io.ddbm.pc.MockContextService"/>
</beans>