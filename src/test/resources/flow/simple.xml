<?xml version="1.0" encoding="utf-8"?>
<flow name="simple" context="mockContextService">
    <start name="init">
        <cmd name="push" action="initAction" to="prepay"></cmd>
    </start>
    <end name="su"></end>
    <end name="fail"></end>
    <end name="cancel"></end>
    <node name="prepay">
        <cmd name="push" action="payAction" failNode="unknow" router="payRouter"></cmd>
    </node>
    <node name="unknow" pause="true" retry="3">
        <cmd name="push" action="queryAction" failNode="unknow" router="queryRouter"></cmd>
    </node>

    <routers>
        <router name="payRouter">
            <expression test="" to=""></expression>
            <expression test="" to=""></expression>
            <expression test="" to=""></expression>
        </router>
        <router name="queryRouter">
            <expression test="" to=""></expression>
            <expression test="" to=""></expression>
            <expression test="" to=""></expression>
        </router>

    </routers>
</flow>